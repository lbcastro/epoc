<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contacts_relative1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/clean_pondwater" >

    <RelativeLayout
        android:id="@+id/contacts_relative2"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" >

        <ImageView
            android:id="@+id/contact3"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dip"
            android:alpha="1.0"
            android:src="@drawable/contact3" />

        <ImageView
            android:id="@+id/contact4"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignTop="@id/contact3"
            android:layout_marginRight="10dip"
            android:layout_toRightOf="@id/contact3"
            android:alpha="0.5"
            android:src="@drawable/contact4" />

        <ImageView
            android:id="@+id/contact5"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignTop="@id/contact3"
            android:layout_toEndOf="@id/contact4"
            android:alpha="0.5"
            android:src="@drawable/contact5" />

        <ImageView
            android:id="@+id/contact1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="120dp"
            android:layout_toLeftOf="@id/contact3"
            android:alpha="0.5"
            android:src="@drawable/contact1" />

        <ImageView
            android:id="@+id/contact2"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignTop="@id/contact3"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dip"
            android:layout_toLeftOf="@id/contact3"
            android:alpha="0.5"
            android:src="@drawable/contact2" />

        <TextView
            android:id="@+id/name3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/contact3"
            android:layout_alignRight="@id/contact3"
            android:layout_below="@id/contact3"
            android:layout_marginTop="-48dip"
            android:alpha="1.0"
            android:ems="10"
            android:enabled="false"
            android:shadowColor="#000000"
            android:shadowDx="1"
            android:shadowDy="1"
            android:shadowRadius="1"
            android:text="Barack Obama"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/name4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/contact4"
            android:layout_alignRight="@id/contact4"
            android:layout_below="@id/contact4"
            android:layout_marginTop="-48dip"
            android:alpha="0.5"
            android:ems="10"
            android:enabled="false"
            android:shadowColor="#000000"
            android:shadowDx="1"
            android:shadowDy="1"
            android:shadowRadius="1"
            android:text="Angelina Jolie"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/name5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/contact5"
            android:layout_alignRight="@id/contact5"
            android:layout_below="@id/contact5"
            android:layout_marginTop="-48dip"
            android:alpha="0.5"
            android:ems="10"
            android:enabled="false"
            android:shadowColor="#000000"
            android:shadowDx="1"
            android:shadowDy="1"
            android:shadowRadius="1"
            android:text="Steve Jobs"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/contact1"
            android:layout_alignRight="@id/contact1"
            android:layout_below="@id/contact1"
            android:layout_marginTop="-48dip"
            android:alpha="0.5"
            android:ems="10"
            android:enabled="false"
            android:shadowColor="#000000"
            android:shadowDx="1"
            android:shadowDy="1"
            android:shadowRadius="1"
            android:text="Tom Cruise"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/name2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/contact2"
            android:layout_alignRight="@id/contact2"
            android:layout_below="@id/contact2"
            android:layout_marginTop="-48dip"
            android:alpha="0.5"
            android:ems="10"
            android:enabled="false"
            android:shadowColor="#000000"
            android:shadowDx="1"
            android:shadowDy="1"
            android:shadowRadius="1"
            android:text="Marilyn Monroe"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/gyrotext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/contact3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/rock_my_world" />

    </RelativeLayout>

</RelativeLayout>